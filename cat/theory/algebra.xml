
<gat:algebra 
    xmlns:cat              ="http://www.entitymodelling.org/theory/category"
    xmlns:gat              ="http://www.entitymodelling.org/theory/generalisedalgebraictheory" 	   
    xmlns="http://www.entitymodelling.org/theory/category" >

  <gat:id>cat</gat:id>	
  <gat:name>Theory of categories</gat:name>  
  <gat:namespace>http://www.entitymodelling.org/theory/gat:category</gat:namespace>

  <gat:sort>
    <gat:id>Ob</gat:id>
    <gat:context>
    </gat:context>
    <gat:isatype>
      <Ob/>
    </gat:isatype>
  </gat:sort>

  <gat:sort>
    <gat:id>Hom</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>y</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
    </gat:context>
    <gat:isatype>
      <Hom><var>x</var><var>y</var></Hom>
    </gat:isatype>
  </gat:sort>

  <gat:operator>
    <gat:id>id</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
    </gat:context>
    <gat:term>
      <id><var>x</var></id>
    </gat:term>
    <gat:hastype>
       <Hom><var>x</var><var>y</var></Hom>
    </gat:hastype>
  </gat:operator>
  
  <gat:operator>
    <gat:id>o</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
            <gat:decl>
        <gat:name>y</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>z</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>f</gat:name>
        <gat:type>
          <Hom><var>x</var><var>y</var></Hom>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>g</gat:name>
        <gat:type>
          <Hom><var>y</var><var>z</var></Hom>
        </gat:type>
      </gat:decl>    
    </gat:context>
    <gat:term>
      <o><var>x</var><var>y</var><var>z</var><var>f</var><var>g</var></o>
    </gat:term>
    <gat:hastype>
       <Hom><var>x</var><var>y</var></Hom>
    </gat:hastype>
  </gat:operator>

  <gat:equation>
    <gat:id>1</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>y</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>f</gat:name>
        <gat:type>
          <Hom>
            <var>x</var>
            <var>y</var>
          </Hom>
        </gat:type>
      </gat:decl>
    </gat:context>
    <gat:lhs>
      <o>
        <id><var>x</var></id>
        <var>f</var>
      </o>
    </gat:lhs>
    <gat:rhs>
      <var>f</var>
    </gat:rhs>
  </gat:equation>

  <gat:equation>
    <gat:id>2</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>y</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>f</gat:name>
        <gat:type>
          <Hom>
            <var>x</var>
            <var>y</var>
          </Hom>
        </gat:type>
      </gat:decl>
    </gat:context>
    <gat:lhs>
      <o>
        <var>f</var>
        <id><var>y</var></id>
      </o>
    </gat:lhs>
    <gat:rhs>
      <var>f</var>
    </gat:rhs>
  </gat:equation>

  <gat:equation>
    <gat:id>3</gat:id>
    <gat:context>
      <gat:decl>
        <gat:name>w</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>x</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>y</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>z</gat:name>
        <gat:type>
          <Ob/>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>f</gat:name>
        <gat:type>
          <Hom>
            <var>w</var>
            <var>x</var>
          </Hom>
        </gat:type>
      </gat:decl>
      <gat:decl>
        <gat:name>g</gat:name>
        <gat:type>
          <Hom>
            <var>x</var>
            <var>y</var>
          </Hom>
        </gat:type>
      </gat:decl>	
      <gat:decl>
        <gat:name>h</gat:name>
        <gat:type>
          <Hom>
            <var>y</var>
            <var>z</var>
          </Hom>
        </gat:type>
      </gat:decl>			
    </gat:context>
    <gat:lhs>
      <o>
        <var>f</var>
        <o>
          <var>g</var>
          <var>h</var>
        </o>
      </o>
    </gat:lhs>
    <gat:rhs>
      <o>
        <o>
          <var>f</var>
          <var>g</var>
        </o>
        <var>h</var>
      </o>
    </gat:rhs>
  </gat:equation>

</gat:algebra>
