               </var>
            </g>
         </gat:term>
         <gat:type>
            <C/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:example>
   <gat:example>
      <gat:id>expr3</gat:id>
      <gat:from>
         <gat:ref>expr2</gat:ref>
         <gat:ref>axiom</gat:ref>
      </gat:from>
      <gat:context>
         <gat:decl>
            <gat:name>y</gat:name>
            <gat:type>
               <B>
                  <f>
                     <a_1/>
                  </f>
               </B>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tT-conclusion>
         <gat:term>
            <g>
               <f>
                  <a_2/>
               </f>
               <var>
                  <gat:name>y</gat:name>
                  <gat:type>
                     <B>
                        <f>
                           <a_1/>
                        </f>
                     </B>
                  </gat:type>
               </var>
            </g>
         </gat:term>
         <gat:type>
            <Ap/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:example>
   <gat:example>
      <gat:id>equality</gat:id>
      <gat:negative/>
      <gat:context>
         <gat:decl>
            <gat:name>y</gat:name>
            <gat:type>
               <B>
                  <f>
                     <a_1/>
                  </f>
               </B>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tt-conclusion>
         <gat:lhs>
            <g id="g1" context="self::*">
               <f id="f2" context="$g1/child::*[1][self::f]">
                  <a_1 id="a_12"
                       context="$f2/child::*[1][self::a_1][not(following-sibling::*)][not(child::*)]"/>
               </f>
               <var>
                  <gat:name>y</gat:name>
                  <gat:type>
                     <B>
                        <f>
                           <a_1/>
                        </f>
                     </B>
                  </gat:type>
               </var>
               <gat:type id="gat:type2"
                         context="$f2 y_1/following-sibling::*[1][self::gat:type][not(following-sibling::*)]">
                  <Ap id="Ap1"
                      context="$gat:type2/child::*[1][self::Ap][not(following-sibling::*)][not(child::*)]"/>
               </gat:type>
            </g>
         </gat:lhs>
         <gat:rhs>
            <g>
               <f>
                  <a_2/>
               </f>
               <var>
                  <gat:name>y</gat:name>
                  <gat:type>
                     <B>
                        <f>
                           <a_1/>
                        </f>
                     </B>
                  </gat:type>
               </var>
               <gat:type>
                  <Ap/>
               </gat:type>
            </g>
         </gat:rhs>
      </gat:tt-conclusion>
   </gat:example>
</gat:algebra>
