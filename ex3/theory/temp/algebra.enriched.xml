      </gat:T-conclusion>
   </gat:sort>
   <gat:operator>
      <gat:id>f</gat:id>
      <gat:context>
         <gat:decl>
            <gat:name>x</gat:name>
            <gat:type>
               <A/>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tT-conclusion>
         <gat:term>
            <f>
               <var>
                  <gat:name>x</gat:name>
               </var>
            </f>
         </gat:term>
         <gat:type>
            <Ap/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:operator>
   <gat:sort>
      <gat:id>C</gat:id>
      <gat:context/>
      <gat:T-conclusion>
         <gat:type>
            <C/>
         </gat:type>
      </gat:T-conclusion>
   </gat:sort>
   <gat:sort>
      <gat:id>B</gat:id>
      <gat:context>
         <gat:decl>
            <gat:name>x</gat:name>
            <gat:type>
               <Ap/>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:T-conclusion>
         <gat:type>
            <B>
               <var>
                  <gat:name>x</gat:name>
               </var>
            </B>
         </gat:type>
      </gat:T-conclusion>
   </gat:sort>
   <gat:operator>
      <gat:id>g</gat:id>
      <gat:context>
         <gat:decl>
            <gat:name>x</gat:name>
            <gat:type>
               <A/>
            </gat:type>
         </gat:decl>
         <gat:decl>
            <gat:name>y</gat:name>
            <gat:type>
               <B>
                  <var>
                     <gat:name>x</gat:name>
                  </var>
               </B>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tT-conclusion>
         <gat:term>
            <f>
               <var>
                  <gat:name>x</gat:name>
               </var>
               <var>
                  <gat:name>y</gat:name>
               </var>
            </f>
         </gat:term>
         <gat:type>
            <C/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:operator>
   <gat:operator>
      <gat:id>a_1</gat:id>
      <gat:context/>
      <gat:tT-conclusion>
         <gat:term>
            <a_1/>
         </gat:term>
         <gat:type>
            <A/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:operator>
   <gat:operator>
      <gat:id>a_2</gat:id>
      <gat:context/>
      <gat:tT-conclusion>
         <gat:term>
            <a_2/>
         </gat:term>
         <gat:type>
            <A/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:operator>
   <gat:equation>
      <gat:id>axiom</gat:id>
      <gat:context/>
      <gat:tt-conclusion>
         <gat:lhs>
            <f id="f1" context="self::*">
               <a_1 id="a_11"
                    context="$f1/child::*[1][self::a_1][not(following-sibling::*)][not(child::*)]"/>
            </f>
         </gat:lhs>
         <gat:rhs>
            <f>
               <a_2/>
            </f>
         </gat:rhs>
      </gat:tt-conclusion>
   </gat:equation>
   <gat:example>
      <gat:id>expr1</gat:id>
      <gat:from>
         <gat:ref>f</gat:ref>
         <gat:ref>a_1</gat:ref>
      </gat:from>
      <gat:context>
         <gat:decl>
            <gat:name>y</gat:name>
            <gat:type>
               <B>
                  <f>
                     <a_1/>
                  </f>
               </B>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tT-conclusion>
         <gat:term>
            <g>
               <a_1/>
               <var>
                  <gat:name>y</gat:name>
                  <gat:type>
                     <B>
                        <f>
                           <a_1/>
                        </f>
                     </B>
                  </gat:type>
               </var>
            </g>
         </gat:term>
         <gat:type>
            <C/>
         </gat:type>
      </gat:tT-conclusion>
   </gat:example>
   <gat:example>
      <gat:id>expr2</gat:id>
      <gat:from>
         <gat:ref>f</gat:ref>
         <gat:ref>a_2</gat:ref>
      </gat:from>
      <gat:context>
         <gat:decl>
            <gat:name>y</gat:name>
            <gat:type>
               <B>
                  <f>
                     <a_2/>
                  </f>
               </B>
            </gat:type>
         </gat:decl>
      </gat:context>
      <gat:tT-conclusion>
         <gat:term>
            <g>
               <f>
                  <a_2/>
               </f>
               <var>
                  <gat:name>y</gat:name>
                  <gat:type>
                     <B>
                        <f>
                           <a_2/>
                        </f>
                     </B>
