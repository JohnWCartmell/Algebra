	<gat:rewriteRule>
		<gat:id>C.1</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:sequence>
					<gat:name>f_1</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_0</gat:name>
							</var>
							<var>
								<gat:name>x_1</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>f_2</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_1</gat:name>
							</var>
							<var>
								<gat:name>x_2</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>f_3</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_2</gat:name>
							</var>
							<var>
								<gat:name>x_3</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<o>
						<var>x_0</var>
						<seq>f_1</seq>
						<o>
							<var>x_1</var>
							<seq>f_2</seq>
						</o>
						<seq>f_3</seq>
					</o>
				</gat:lhs>
				<gat:rhs>
					<o>
						<var>x_0</var>
						<seq>f_1</seq>
						<seq>f_2</seq>
						<seq>f_3</seq>
					</o>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:rewriteRule>

