<?xml version="1.0"?>
<!-- 22 Feb 2017 -->
<gat:algebra xmlns:ccseq="http://www.entitymodelling.org/theory/contextualcategory/sequence" xmlns:gat="http://www.entitymodelling.org/theory/generalisedalgebraictheory" xmlns="http://www.entitymodelling.org/theory/contextualcategory/sequence">

	<gat:name>ccseq</gat:name>
	<gat:namespace>http://www.entitymodelling.org/theory/contextualcategory/sequence</gat:namespace>
	
		<gat:sort>
		<gat:id>Ob</gat:id>
		<gat:T-rule>
			<gat:context>
			</gat:context>
			<gat:T-conclusion>
				<gat:type>
					<Ob/>
				</gat:type>
			</gat:T-conclusion>
		</gat:T-rule>
	</gat:sort>

	<gat:sort>
		<gat:id>Hom</gat:id>
		<gat:T-rule>
			<gat:context>
				<gat:decl>
					<gat:name>x</gat:name>
					<gat:type>
						<Ob/>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob/>
					</gat:type>
				</gat:decl>
			</gat:context>
			<gat:T-conclusion>
				<gat:type>
					<Hom><var>x</var><var>y</var></Hom>
				</gat:type>
			</gat:T-conclusion>
		</gat:T-rule>
	</gat:sort>


	<gat:operator>
		<gat:id>o</gat:id>
		<gat:tT-rule>
			<gat:context>
				<gat:decl>
					<gat:name>x</gat:name>
					<gat:type>
						<Ob/>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob/>
					</gat:type>
				</gat:decl>
				<gat:sequence>
					<gat:name>f</gat:name>
					<gat:type>
						<HomSeq><var>x</var><var>y</var></HomSeq>
					</gat:type>
				</gat:sequence>
			</gat:context>
			<gat:tT-conclusion>
				<gat:term>
					<o><var>x</var><seq>f</seq></o>
				</gat:term>
				<gat:type>
					<Hom><var>x</var><var>y</var></Hom>
				</gat:type>
			</gat:tT-conclusion>
		</gat:tT-rule>
	</gat:operator>
	

	<gat:axiom>
		<gat:id>C.1</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:sequence>
					<gat:name>f_1</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_0</gat:name>
							</var>
							<var>
								<gat:name>x_1</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>f_2</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_1</gat:name>
							</var>
							<var>
								<gat:name>x_2</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>f_3</gat:name>
					<gat:type>
						<HomSeq>
							<var>
								<gat:name>x_2</gat:name>
							</var>
							<var>
								<gat:name>x_3</gat:name>
							</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<o>
						<var>x_0</var>
						<seq>f_1</seq>
						<o>
							<var>x_1</var>
							<seq>f_2</seq>
						</o>
						<seq>f_3</seq>
					</o>
				</gat:lhs>
				<gat:rhs>
					<o>
						<var>x_0</var>
						<seq>f_1</seq>
						<seq>f_2</seq>
						<seq>f_3</seq>
					</o>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>

	<gat:axiom>
		<gat:id>C.2</gat:id>
		<gat:tt-rule>
			<gat:context> 
				<gat:decl>
					<gat:name>f</gat:name>
					<gat:type>
						<Hom>
							<var>
								<gat:name>x</gat:name>
							</var>
							<var>
								<gat:name>y</gat:name>
							</var>
						</Hom>
					</gat:type>
				</gat:decl>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<o>
						<var>x</var>
						<var>f</var>
					</o>
				</gat:lhs>
				<gat:rhs>
					<var>f</var>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>

	<gat:axiom>
		<gat:id>C0.0</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob>
							<var>x</var>
						</Ob>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>f</gat:name>
					<gat:type>
						<Hom>
							<var>w</var>
							<var>x</var>
						</Hom>
					</gat:type>
				</gat:decl>
				<gat:sequence>
					<gat:name>h_1</gat:name>
					<gat:type>
						<HomSeq>
							<var>v</var>
							<star>
								<var>w</var>
								<var>f</var>
								<var>y</var>
							</star>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>h_2</gat:name>
					<gat:type>
						<HomSeq>
							<var>x</var>
							<var>z</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<o>
						<var>v</var>
						<seq>h_1</seq>
						<q>
							<var>w</var>
							<var>f</var>
							<var>y</var>
						</q>
						<p>
							<var>y</var>
							<var>x</var>
						</p>
						<seq>h_2</seq>
					</o>
				</gat:lhs>
				<gat:rhs>
					<o>
						<var>v</var>
						<seq>h_1</seq>
						<p>
							<star>
								<var>w</var>
								<var>f</var>
								<var>y</var>
							</star>
							<var>w</var>
						</p>
						<var>f</var>
						<seq>h_2</seq>
					</o>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>

	<gat:axiom>
		<gat:id>C0.1</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob>
							<var>x</var>
						</Ob>
					</gat:type>
				</gat:decl>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>      
					<star>
						<var>x</var>
						<o>
							<var>x</var>
						</o>
						<var>y</var>
					</star>       
				</gat:lhs>
				<gat:rhs>
					<var>y</var>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>

	<gat:axiom>
		<gat:id>C0.2</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob>
							<var>x</var>
						</Ob>
					</gat:type>
				</gat:decl>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>      
					<q>
						<var>x</var>
						<o>
							<var>x</var>
						</o>
						<var>y</var>
					</q>       
				</gat:lhs>
				<gat:rhs>
					<o>
						<var>y</var>
					</o>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>


	<gat:axiom>
		<gat:id>C0.3</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:decl>
					<gat:name>z</gat:name>
					<gat:type>
						<Ob>
							<var>y</var>
						</Ob>
					</gat:type>
				</gat:decl>

				<gat:decl>
					<gat:name>f</gat:name>
					<gat:type>
						<Hom>
							<var>w</var>
							<var>x</var>
						</Hom>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>g</gat:name>
					<gat:type>
						<Hom>
							<var>x</var>
							<var>y</var>
						</Hom>
					</gat:type>
				</gat:decl>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<star>
						<var>w</var>
						<var>f</var>
						<star>
							<var>x</var>
							<var>g</var>
							<var>z</var>
						</star>
					</star>
				</gat:lhs>
				<gat:rhs>
					<star>
						<var>w</var>
						<o>
							<var>w</var>
							<var>f</var>
							<var>g</var>
						</o>
						<var>z</var>
					</star>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>

	<gat:axiom>
		<gat:id>C0.4</gat:id>
		<gat:tt-rule>
			<gat:context>
				<gat:decl>
					<gat:name>xx</gat:name>
					<gat:type>
						<Ob>
							<var>x</var>
						</Ob>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>w</gat:name>
					<gat:type>
						<Ob>
							<var>w_p</var>
						</Ob>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>y</gat:name>
					<gat:type>
						<Ob>
							<var>y_p</var>
						</Ob>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>f</gat:name>
					<gat:type>
						<Hom>
							<var>w</var>
							<var>x</var>
						</Hom>
					</gat:type>
				</gat:decl>
				<gat:decl>
					<gat:name>g</gat:name>
					<gat:type>
						<Hom>
							<var>x</var>
							<var>y_p</var>
						</Hom>
					</gat:type>
				</gat:decl>
				<gat:sequence>
					<gat:name>h_1</gat:name>
					<gat:type>
						<HomSeq>
							<var>v</var>
							<star>
								<var>w</var>
								<var>f</var>
								<star>
									<var>x</var>
									<var>g</var>
									<var>y</var>
								</star>
							</star>
						</HomSeq>
					</gat:type>
				</gat:sequence>
				<gat:sequence>
					<gat:name>h_2</gat:name>
					<gat:type>
						<HomSeq>
							<var>y</var>
							<var>z</var>
						</HomSeq>
					</gat:type>
				</gat:sequence>
			</gat:context>
			<gat:tt-conclusion>
				<gat:lhs>
					<o>
						<var>v</var>
						<seq>h_1</seq>
						<q>
							<var>w</var>
							<var>f</var>
							<var>xx</var>
						</q>
						<q>
							<var>x</var>
							<var>g</var>
							<var>y</var>
						</q>
						<seq>h_2</seq>
					</o>
				</gat:lhs>
				<gat:rhs>
					<o>
						<var>v</var>
						<seq>h_1</seq>
						<q>
							<var>w</var>
							<o>
								<var>w</var>
								<var>f</var>
								<var>g</var>
							</o>
							<var>y</var>
						</q>
						<seq>h_2</seq>
					</o>
				</gat:rhs>
			</gat:tt-conclusion>
		</gat:tt-rule>
	</gat:axiom>


</gat:algebra>
